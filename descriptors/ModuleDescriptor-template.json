{
  "id": "edge-ncip-0.0.1",
  "name": "Edge NCIP",
    "provides": [
    {
      "id": "edge-ncip",
      "version": "0.0.1",
      "handlers": [
        {
          "methods": [ "*"],
          "pathPattern": "/circapi/*"
        }
      ]
    }
  ],
  "requires": [],
  "permissionSets": [],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerArgs": {
      "HostConfig": { "PortBindings": { "8081/tcp":  [{ "HostPort": "%p" }] } }
    },
    "dockerPull" : false
  }
}
